{
    "name": "renoki-co/php-helm",
    "description": "PHP Helm is a process wrapper for Kubernetes' Helm v3",
    "keywords": ["laravel", "php"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/php-helm",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpHelm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpHelm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0",
        "orchestra/database": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\PhpHelm\\PhpHelmServiceProvider"
            ]
        }
    }
}
